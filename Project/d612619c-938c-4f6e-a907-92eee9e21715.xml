Sub OnOpen&#xD;
	&#xD;
	' Do initalitation only one time&#xD;
	If User.Sta.Initalized Then&#xD;
		Exit Sub&#xD;
	Else&#xD;
		User.Sta.Initalized = True&#xD;
	End If&#xD;
	&#xD;
	' Subscribe to an update of the user list&#xD;
	AddHandler S_Users.OnListUpdate, AddressOf _ListUpdate&#xD;
	&#xD;
	' Set change of user to user manager&#xD;
	AddHandler Me.List.OnChangeDatas, AddressOf S_Users.ListChange&#xD;
	&#xD;
	' Create liste of user&#xD;
	Call S_Users.SetList&#xD;
	&#xD;
End Sub&#xD;
&#xD;
''' &lt;summary&gt;&#xD;
''' Update list viewer from datas in S_Users&#xD;
''' &lt;/summary&gt;&#xD;
Private Sub _ListUpdate&#xD;
	Me.List.SetList(S_Users.ListView, S_Users.ListData)&#xD;
End Sub&#xD;
